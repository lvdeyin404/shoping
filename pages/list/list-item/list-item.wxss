/* pages/list/list-item/list-item.wxss */
html body{
  width: 100%;
}

.box{
  background: #EAEAEA;
}

/* banenr */
.banner{
  width: 100%;
}

.banner swiper{
  width: 100%;
  height: 650rpx;
}

.banner swiper-item{
  width: 100%;
}

.banner image{
  width: 100%;
  height: 100%;
}
/* banner 结束 */

/* middle */
.middle{
  width: 95%;
  padding: 0 20rpx;
  background: #ffffff;
}

.middle .middle-title{
  width: 100%;
}
.middle .middle-title .title-text{
  color: #5D5D5D;
}
.middle .middle-title .title-status{
  float: right;
  /* line-height: 80rpx; */
  font-size: 25rpx;
  color: #9FD4AB;
  padding: 5rpx 15rpx;
  border: solid 1rpx #9FD4AB;
  border-radius: 5px;
}
.middle .middle-duction{
  height: auto;
  line-height: 50rpx;
  font-size: 28rpx;
  color: #BABABA;
}
.middle .time{
  margin-top: 10rpx;
  font-size: 25rpx;
  color: #9E9E9E;
}
.middle .time text{
  margin-left: 10rpx;
}
.middle .people-number{
  margin-top: 15rpx;
  font-size: 25rpx;
}
.middle .people-number .people-one{
  color: #FF5757;
}
.middle .people-number .people-one text{
  margin-left: 10rpx;
}
.middle .people-number .people-two{
  float: right;
   color: #9E9E9E;
}
.middle .people-number .people-two text{
  color: #FF5757;
  margin-left: 10rpx;
}
.middle .speed{
  border-radius: 5px;
  margin-top: 15rpx;
  width: 100%;
  background: #EAEAEA;
  height: 13rpx;
}
.middle .speed .speed-item{
  border-radius: 15rpx;
  background: #FF4E3D;
  height: 13rpx;
}
.middle .is_status{
  color: #BABABA;
  font-size: 26rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}

.fix{
  height: 80rpx;
  width: 100%;
  line-height: 80rpx;
  font-size: 28rpx;
  text-align: center;
  background: #FF5757;
  color: #ffffff;
  position: fixed;
  bottom: 0;
}
/* middle end */

/* buttom */
.buttom{
  margin-top: 30rpx;
  width: 100%;
  margin-bottom: 85rpx;
  color: #5D5D5D;
}
.buttom span{
  margin-left: 72%;
}
.buttom .muan1{
  width: 95%;
  padding: 0 20rpx;
  background: #ffffff;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: solid 1rpx #EAEAEA;
}
.buttom .muan2{
  width: 95%;
  padding: 0 20rpx;
  background: #ffffff;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: solid 1rpx #EAEAEA;
}